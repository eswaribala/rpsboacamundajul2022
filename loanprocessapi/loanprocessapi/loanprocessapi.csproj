<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>b98e2baf-a805-41f7-b069-deb131bd4c2d</UserSecretsId>
    <DockerDefaultTargetOS>Linux</DockerDefaultTargetOS>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Decisions\loansanctionrules.dmn" />
    <None Remove="Forms\LoanAcceptanceForm.html" />
    <None Remove="Forms\LoanForm.html" />
    <None Remove="Forms\LoginForm.html" />
    <None Remove="Forms\Task-Form-PDF-Additional-Upload.html" />
    <None Remove="Forms\Task-Form-PDF-Upload.html" />
    <None Remove="Forms\Task-Form-PDF-UW-Upload.html" />
    <None Remove="Forms\Task-Form-PDF-Viewer.html" />
    <None Remove="Processes\loanprocess.bpmn" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Decisions\loansanctionrules.dmn">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </EmbeddedResource>
    <EmbeddedResource Include="Forms\LoanAcceptanceForm.html">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </EmbeddedResource>
    <EmbeddedResource Include="Forms\LoanForm.html">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </EmbeddedResource>
    <EmbeddedResource Include="Forms\LoginForm.html">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </EmbeddedResource>
    <EmbeddedResource Include="Forms\Task-Form-PDF-Additional-Upload.html">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </EmbeddedResource>
    <EmbeddedResource Include="Forms\Task-Form-PDF-UW-Upload.html">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </EmbeddedResource>
    <EmbeddedResource Include="Forms\Task-Form-PDF-Upload.html">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </EmbeddedResource>
    <EmbeddedResource Include="Forms\Task-Form-PDF-Viewer.html">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </EmbeddedResource>
    <EmbeddedResource Include="Processes\loanprocess.bpmn">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </EmbeddedResource>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Camunda.Api.Client" Version="2.6.0" />
    <PackageReference Include="Camunda.Worker" Version="0.13.4" />
    <PackageReference Include="Microsoft.VisualStudio.Azure.Containers.Tools.Targets" Version="1.14.0" />
    <PackageReference Include="Swashbuckle.AspNetCore" Version="6.2.3" />
  </ItemGroup>

</Project>
